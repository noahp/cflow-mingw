AT_SETUP([same type declaration])
AT_KEYWORDS([decl decl01])

CFLOW_OPT([--debug],[
CFLOW_CHECK([static struct X *a, *b, *c;],
[],
[prog:1: a/-1 defined to struct X *a
prog:1: b/-1 defined to struct X *b
prog:1: c/-1 defined to struct X *c
])])


AT_SETUP([direct tree])
AT_KEYWORDS([direct])

CFLOW_CHECK([
foo()
{
}

bar()
{
     foo();
}

main()
{
     bar();
     foo();
}
],
[main() <main () at prog:11>:
    bar() <bar () at prog:6>:
        foo() <foo () at prog:2>
    foo() <foo () at prog:2>
])


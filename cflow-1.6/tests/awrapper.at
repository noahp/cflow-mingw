AT_SETUP([attribute wrapper handling])
AT_KEYWORDS([awrapper])

CFLOW_OPT([--symbol __attribute__:wrapper],[

# Early versions of cflow coredumped on this output
CFLOW_CHECK([
extern main() __attribute__((noreturn));

main()
{
     foo();
}
],
[main() <main () at prog:4>:
    foo()
])

])

AT_CLEANUP




AT_SETUP([forward function declarations])
AT_KEYWORDS([fdecl])

CFLOW_OPT([-v],[
CFLOW_CHECK([
int foo(int x,int y), bar(long u);

int
foo(int x, int y)
{
     bar();
}
],
[foo() <int foo (int x, int y) at prog:5>:
    bar()
])
])

AT_CLEANUP
